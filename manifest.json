{
  "manifest_version": 2,

  "name": "Bookmarks in google results",
  "description": "This extension show the bookmarks that match your search keywords at the top of the Google results",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "jquery.min.js",
        "jquery.ba-throttle-debounce.min.js", // https://github.com/cowboy/jquery-throttle-debounce
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.google.fr/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "bookmarks"
  ]
}
